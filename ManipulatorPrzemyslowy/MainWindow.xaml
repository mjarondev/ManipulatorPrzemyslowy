<Window x:Class="ManipulatorPrzemyslowy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManipulatorPrzemyslowy"
        mc:Ignorable="d"
        Title="MainWindow" Height="237" Width="220" Closed="Window_Closed">
    <StackPanel>
        <Button x:Name="CommunicationPortButton" Content="Communication Port" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" FontSize="22" Click="CommunicationPortButton_Click"/>
        <Button Content="Command tool" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" FontSize="22"/>
        <Button Content="Jog operator" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" FontSize="22"/>
        <Label Name="testLbl" Content="Label" Height="35"/>
        <Button x:Name="ConnectButton" Content="Connect" FontSize="22" Click="ConnectButton_Click"/>
        <Button x:Name="SendButton" Content="Send" FontSize="22" Click="SendButton_Click"/>
    </StackPanel>
</Window>
