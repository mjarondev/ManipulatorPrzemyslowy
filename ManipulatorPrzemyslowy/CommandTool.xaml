<Window x:Class="ManipulatorPrzemyslowy.CommandTool"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManipulatorPrzemyslowy"
        mc:Ignorable="d"
        Title="CommandTool" Height="400" Width="480" Closed="Window_Closed">
    <Grid>
        <ListBox x:Name="CommandList" Margin="10,47,0,0" Width="232" Height="200" VerticalAlignment="Top" UseLayoutRounding="False" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Visible" HorizontalAlignment="Left" MouseDoubleClick="CommandList_MouseDoubleClick" SelectionChanged="CommandList_SelectionChanged"/>
        <Label x:Name="SyntaxLbl" Content="" HorizontalAlignment="Center" Margin="0,252,0,0" VerticalAlignment="Top" Width="460" Height="110"/>
        <TextBox x:Name="CommandTxtBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232" Height="27"/>
        <Label Content="connection info&#xD;&#xA;&#xD;&#xA;" HorizontalAlignment="Left" Margin="271,10,0,327" Width="160"/>
        <Button Content="Send" HorizontalAlignment="Left" Margin="271,42,0,0" VerticalAlignment="Top" Height="28" Width="160"/>
        <Label Content="robot info" HorizontalAlignment="Left" Margin="271,117,0,0" VerticalAlignment="Top" Width="160" Height="130"/>
        <Label Content="Data from robot" HorizontalAlignment="Left" Margin="271,85,0,0" VerticalAlignment="Top" Width="160" Height="27"/>

    </Grid>
</Window>
